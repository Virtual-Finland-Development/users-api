{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.EntityFrameworkCore.Database.Command": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "Security": {
    "Authorization": {
      "Testbed": {
        "IsEnabled": true
      },
      "Sinuna": {
        "IsEnabled": true
      },
      "SuomiFi": {
        "IsEnabled": true
      }
    },
    "Options": {
      "TermsOfServiceAgreementRequired": true
    }
  },
  "Services": {
    "Codesets": {
      "ServiceRequestTimeoutInMilliseconds": 30000
    }
  }
}