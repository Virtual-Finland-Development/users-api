{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Security": {
    "Authorization": {
      "Testbed": {
        "IsEnabled": true
      },
      "Sinuna": {
        "IsEnabled": true
      },
      "SuomiFi": {
        "IsEnabled": true
      }
    },
    "Options": {
      "TermsOfServiceAgreementRequired": true
    }
  }
}